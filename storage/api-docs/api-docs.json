{
    "openapi": "3.0.0",
    "info": {
        "title": "Proyecto Android Laravel",
        "version": "1.0"
    },
    "servers": [
        {
            "url": "http://192.168.0.9:8000"
        }
    ],
    "paths": {
        "/api/login": {
            "get": {
                "summary": "Inicar sesion",
                "operationId": "App\\Http\\Controllers\\UserController::inicioSesion",
                "responses": {
                    "200": {
                        "description": "Iniciar sesion."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        },
        "/api/actualizarDatos": {
            "put": {
                "summary": "actualizarDatos",
                "operationId": "App\\Http\\Controllers\\UserController::actualizarDatos",
                "responses": {
                    "200": {
                        "description": "Se actualizan los datos del usuario logeado."
                    },
                    "default": {
                        "description": "Ha ocurrido un error."
                    }
                }
            }
        }
    }
}